<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="154.0" prefWidth="297.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox alignment="CENTER" prefHeight="124.0" prefWidth="103.0" spacing="5.0" BorderPane.alignment="CENTER_LEFT">
         <children>
            <HBox alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="175.0" spacing="10.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nom :" />
                  <TextField fx:id="textNom" promptText="Saisir..." />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="175.0" spacing="10.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Adresse :" />
                  <TextField fx:id="textAdresse" promptText="Saisir..." />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="175.0" spacing="10.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Téléphone :" />
                  <TextField fx:id="textTelephone" promptText="Saisir..." />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="175.0" spacing="10.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Email :" />
                  <TextField fx:id="textEmail" promptText="Saisir..." />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
   <bottom>
      <HBox prefHeight="35.0" prefWidth="195.0" spacing="50.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="bnSave" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" style="-fx-background-color: green; -fx-text-fill: white;" text="Confirmer" />
            <Button fx:id="bnClose" mnemonicParsing="false" style="-fx-background-color: red; -fx-text-fill: white;" text="Annuler" />
         </children>
         <padding>
            <Insets left="50.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
